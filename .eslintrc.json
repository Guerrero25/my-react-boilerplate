{
  "env": {
    "browser": true,
    "node": false
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "semi-spacing": ["error", { "before": false, "after": true }],
    "prefer-spread": "warn",
    "sort-keys": "error",
    "sort-vars": "error",
    "prefer-object-spread": "warn",
    "global-require": "error",
    "no-undef-init": "warn"
  }
}
